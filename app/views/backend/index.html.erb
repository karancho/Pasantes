<h1>Listando usuarios</h1>

<table>
  <tr>
    <th>Nombre</th>
    <th>Apellido</th>
    <th></th>
    <th></th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.name %></td>
    <td><%= user.surname %></td>

    <td><%#= link_to 'Edit', edit_user_registration_path(user) %></td>
    <td><%= link_to 'Edit', backend_edit_path(user) %></td>

    <td><%#= link_to 'Destroy', user, :method => :delete, :data => { :confirm => 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>


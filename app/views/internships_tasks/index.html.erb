<h1>Listing internships_tasks</h1>

<table>
  <tr>
    <th>Internship</th>
    <th>Task</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @internships_tasks.each do |internships_task| %>
  <tr>
    <td><%= internships_task.internship_id %></td>
    <td><%= internships_task.task_id %></td>
    <td><%= link_to 'Show', internships_task %></td>
    <td><%= link_to 'Edit', edit_internships_task_path(internships_task) %></td>
    <td><%= link_to 'Destroy', internships_task, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

